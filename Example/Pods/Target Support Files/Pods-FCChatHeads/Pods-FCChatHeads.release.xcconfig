CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CMPopTipView" "${PODS_ROOT}/Headers/Public/FCChatHeads" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CMPopTipView" -isystem "${PODS_ROOT}/Headers/Public/FCChatHeads" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-FCChatHeads-CMPopTipView" -l"Pods-FCChatHeads-FCChatHeads" -l"Pods-FCChatHeads-pop" -l"c++" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods